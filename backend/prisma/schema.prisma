datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

model User {
  id          String   @id @default(cuid())
  email       String   @unique
  password    String
  vbucks      Int      @default(10000)
  createdAt   DateTime @default(now())
}
